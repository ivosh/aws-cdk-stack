{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": [ "es2018" ],

    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,

    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
